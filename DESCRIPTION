  httr,
  jsonlite,
  magrittr,
  openssl,
  withr,
  yaml,
  zip
Suggests:
  knitr,
  rmarkdown,
  R6,
  testthat
VignetteBuilder: knitr
URL: https://github.com/rstudio/pins
